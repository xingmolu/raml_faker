{
    "$schema": "http://json-schema.org/schema",
    "type": "object",
    "description": "A collection of objects",
    "properties": {
        "persons": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "name",
                        "value": "findName"
                    },
                    "country": {
                        "type": "address",
                        "value": "country"
                    },
                    "province": {
                        "type": "address",
                        "value": "province"
                    },

                    "abbr": {
                        "type": "address",
                        "value": "stateAbbr"
                    },

                    "city": {
                        "type": "address",
                        "value": "city"
                    },

                    "street": {
                        "type": "address",
                        "value": "streetName"
                    },

                    "phoneNumber": {
                        "type": "phone",
                        "value": "phoneNumber"
                    }
                },
                "required": ["name", "city", "province", "phoneNumber"]
            }
        }
    },
    "required": ["arrayOfObjects"],
    "definitions": {
        "other": {
            "type": "number"
        }
    }
}
